{
    "name": "ESPM",
    "version": "1.0.0",
    "description": "ESPM",
    "repository": "<Add your repository here>",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "^6",
        "express": "^4",
        "hdb": "^0.19.0"
      },
      "devDependencies": {
        "sqlite3": "^5.0.4"
      },
      "scripts": {
        "start": "cds run"
      },
      "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
          "es2020": true,
          "node": true,
          "jest": true,
          "mocha": true
        },
        "globals": {
          "SELECT": true,
          "INSERT": true,
          "UPDATE": true,
          "DELETE": true,
          "CREATE": true,
          "DROP": true,
          "CDL": true,
          "CQL": true,
          "CXL": true,
          "cds": true
        },
        "rules": {
          "no-console": "off",
          "require-atomic-updates": "off"
        }
      },
      "cds": {
        "requires": {
          "db": {
            "kind": "hana-cloud"
          }
        }
      }
}